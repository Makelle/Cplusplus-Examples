infile.open ("c:\c++.txt");
while(infile >> STRING) // To get you all the lines.
        {
	        
//getline(infile,STRING); 
// Saves the line in STRING.
	       
// infile >> STRING;
	        
cout << STRING; // Prints our STRING.

        }
	
